      "commands": [
        "cursorDownSelect",
        "cursorDownSelect",
        "cursorDownSelect",
        "cursorDownSelect",
        "cursorDownSelect",
        "cursorDownSelect",
        "cursorDownSelect",
        "cursorDownSelect",
        "cursorDownSelect",
        "cursorDownSelect"
      ]
    },
    "when": "textInputFocus"
  },
