// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "win+p",
        "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+shift+alt+win+8",
        "command": "-workbench.action.previousEditor"
    },
    { 
        "key": "ctrl+tab",
        "command": "workbench.action.navigateEditorGroups"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "alt+win+right",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "shift+win+t",
        "command": "workbench.action.showAllSymbols"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "ctrl+shift+l",
        "command": "workbench.action.gotoLine"
    },
    {
        "key": "ctrl+s",
        "command": "workbench.action.files.saveAll"
    },
    {
        "key": "ctrl+a",
        "when": "editorTextFocus && !editorReadonly",
        "command": "cursorHome"
    },
    {
        "key": "ctrl+e",
        "when": "editorTextFocus && !editorReadonly",
        "command": "cursorEnd"
    },
    {
        "key": "ctrl+p",
        "command": "cursorUp",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+n",
        "command": "cursorDown",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+e",
        "command": "cursorEnd"
    },
    {
        "key": "win+a",
        "command": "editor.action.selectAll"
    },
    {
        "key": "ctrl+g",
        "command": "editor.action.nextMatchFindAction",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+g",
        "command": "editor.action.previousMatchFindAction",
        "when": "editorFocus"
    },
    { "key": "win+l",                "command": "center-editor-window.center",
        "when": "editorTextFocus" },
        {
            "key": "ctrl+down",
            "command": "spaceBlockJumper.moveDown",
        },
        {
            "key": "ctrl+up",
            "command": "spaceBlockJumper.moveUp",
        },
        {
            "key": "ctrl+shift+down",
            "command": "spaceBlockJumper.selectDown",
        },
        {
            "key": "ctrl+shift+up",
            "command": "spaceBlockJumper.selectUp",
        },
        {
            "key": "ctrl+p",
            "command": "workbench.action.quickOpenSelectPrevious",
            "when": "inQuickOpen"
        },
        {
            "key": "ctrl+n",
            "command": "workbench.action.quickOpenSelectNext",
            "when": "inQuickOpen",
        },
        {
            "key": "ctrl+k",
            "command": "editor.action.deleteLines",
            "when": "textInputFocus",
        },    

        { "key": "ctrl+d",                "command": "editor.action.commentLine",
            "when": "editorTextFocus && !editorReadonly" },
]