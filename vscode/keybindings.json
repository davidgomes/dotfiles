    "key": "win+shift+p",
    "command": "runCommands",
    "args": {
      "commands": [
        "cursorUpSelect",
        "cursorUpSelect",
        "cursorUpSelect",
        "cursorUpSelect",
        "cursorUpSelect",
        "cursorUpSelect",
        "cursorUpSelect",
        "cursorUpSelect",
        "cursorUpSelect",
