  {
    "key": "cmd+n",
    "command": "cursorMove",
    "args": {
      "to": "down",
      "by": "line",
      "value": 10
    },
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+n",
    "command": "runCommands",
