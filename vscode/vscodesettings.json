{
    "editor.minimap.enabled": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.cursorStyle": "block",
    "editor.fontLigatures": false,
    "editor.fontSize": 19,
    "editor.fontFamily": "CommitMono",
    "zenMode.fullScreen": false,
    "window.titleBarStyle": "custom",
    "javascript.validate.enable": false,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.autoReveal": false,
    "git.confirmSync": false,
    "keyboard.dispatch": "keyCode",
    "scrolloff.scrolloff": 7,
    "terminal.integrated.scrollback": 1000000000,
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "window.title": "${folderName} ${separator} ${activeEditorMedium} ${dirty} ${separator} ${appName}",
    "workbench.statusBar.visible": false,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.trimAutoWhitespace": false,
    "editor.renderWhitespace": "all",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.editor.showTabs": "multiple",
    "editor.suggestSelection": "first",
    "breadcrumbs.enabled": false,
    "workbench.sideBar.location": "left",
    "workbench.editorAssociations": {
      "*.ipynb": "jupyter-notebook"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
      "default": "right",
      "jupyter-notebook": "left"
    },
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "extensions.ignoreRecommendations": true,
    "workbench.tree.indent": 15,
    "window.zoomLevel": -1,
    "workbench.colorTheme": "pastel underwater",
    "git.openRepositoryInParentFolders": "never",
    "workbench.activityBar.location": "hidden",
    "editor.inlineSuggest.suppressSuggestions": true,
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDelete": false,
    "workbench.colorCustomizations": {
      // "sideBar.background": "#282C34",
      "list.hoverBackground": "#555555"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDragAndDrop": false
  }
  