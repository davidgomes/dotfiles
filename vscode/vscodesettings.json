{
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.cursorStyle": "block",
  "editor.fontLigatures": false,
  "editor.fontSize": 23,
  "editor.fontFamily": "CommitMono",
  // "editor.fontFamily": "Operator Mono",
  "zenMode.fullScreen": false,
  "window.titleBarStyle": "custom",
  "javascript.validate.enable": false,
  "explorer.autoReveal": false,
  "editor.wordWrap": "on",
  "git.confirmSync": false,
  "keyboard.dispatch": "keyCode",
  "scrolloff.scrolloff": 7,
  "terminal.integrated.scrollback": 1000000000,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "window.title": "${folderName} ${separator} ${activeEditorMedium} ${dirty} ${separator} ${appName}",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.trimAutoWhitespace": false,
  "editor.renderWhitespace": "all",
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.editor.showTabs": "multiple",
  "editor.suggestSelection": "first",
  "breadcrumbs.enabled": false,
  "workbench.sideBar.location": "left",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "extensions.ignoreRecommendations": true,
  "workbench.tree.indent": 15,
  "window.zoomLevel": -1,
  // "workbench.colorTheme": "pastel underwater",
  "workbench.colorTheme": "Pastel Pro",
  "git.openRepositoryInParentFolders": "never",
  "workbench.activityBar.location": "bottom",
  "editor.inlineSuggest.suppressSuggestions": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "workbench.colorCustomizations": {
    "sideBar.background": "#282C34",
    "list.hoverBackground": "#555555",
    // RGBA (Alpha = 0, so no focus border)
    "focusBorder": "#00000000"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "filesFirst",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.newWindowDimensions": "maximized",
  "workbench.startupEditor": "none"
}
