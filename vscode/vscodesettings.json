{
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.cursorStyle": "block",
  "editor.fontLigatures": false,
  "editor.fontSize": 19,
  // "editor.fontFamily": "Iosevka SS04 Light",
  // "editor.fontFamily": "Cascadia Mono Light",
  "zenMode.fullScreen": false,
  "window.titleBarStyle": "custom",
  "javascript.validate.enable": false,
  "explorer.autoReveal": false,
  "editor.wordWrap": "on",
  "git.confirmSync": false,
  "keyboard.dispatch": "keyCode",
  "scrolloff.scrolloff": 7,
  "terminal.integrated.scrollback": 1000000000,
  "terminal.integrated.fontSize": 20,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "window.title": "${folderName} ${separator} ${activeEditorMedium} ${dirty} ${separator} ${appName}",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.trimAutoWhitespace": false,
  "editor.renderWhitespace": "all",
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.editor.showTabs": "multiple",
  "editor.suggestSelection": "first",
  "breadcrumbs.enabled": false,
  "workbench.editorAssociations": {
      "*.ipynb": "jupyter-notebook"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "notebook.cellToolbarLocation": {
      "default": "right",
      "jupyter-notebook": "left"
  },
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "extensions.ignoreRecommendations": true,
  "workbench.tree.indent": 15,
  "window.zoomLevel": -0.25,
  "git.openRepositoryInParentFolders": "never",
  "workbench.activityBar.location": "bottom",
  "editor.inlineSuggest.suppressSuggestions": true,
  "explorer.confirmDelete": false,
  "workbench.colorCustomizations": {
      "sideBar.background": "#282C34",
      "list.hoverBackground": "#555555",
      "focusBorder": "#00000000"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "filesFirst",
  "window.newWindowDimensions": "maximized",
  "cody.commandHints.enabled": false,
  "markdown.preview.fontSize": 25,
  "workbench.startupEditor": "none",
  "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.empty.hint": "hidden",
  "go.toolsManagement.autoUpdate": true,
  "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "zenMode.showTabs": "single",
  "cody.chat.defaultLocation": "sidebar",
  "[python]": {
      "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "chat.commandCenter.enabled": false,
  "workbench.navigationControl.enabled": false,
  "window.commandCenter": false,
  "workbench.colorTheme": "Dracula Pro Edition",
  "haskell.manageHLS": "GHCup"
}