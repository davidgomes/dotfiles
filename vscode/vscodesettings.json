{
  "window.commandCenter": true,
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.cursorStyle": "block",
  "editor.fontLigatures": false,
  "editor.fontSize": 15,
  "editor.fontFamily": "Berkeley Mono",
  "zenMode.fullScreen": false,
  "window.titleBarStyle": "custom",
  "javascript.validate.enable": false,
  "explorer.autoReveal": false,
  "editor.wordWrap": "on",
  "git.confirmSync": false,
  "keyboard.dispatch": "keyCode",
  "scrolloff.scrolloff": 7,
  "terminal.integrated.scrollback": 1000000000,
  "terminal.integrated.fontSize": 15,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "window.title": "${folderName} ${separator} ${activeEditorMedium} ${dirty} ${separator} ${appName}",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.trimAutoWhitespace": false,
  "editor.renderWhitespace": "all",
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.editor.showTabs": "multiple",
  "editor.suggestSelection": "first",
  "breadcrumbs.enabled": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "extensions.ignoreRecommendations": true,
  "workbench.tree.indent": 15,
  "window.zoomLevel": -1,
  "git.openRepositoryInParentFolders": "never",
  "workbench.activityBar.location": "bottom",
  "editor.inlineSuggest.suppressSuggestions": true,
  "explorer.confirmDelete": false,
  "workbench.colorCustomizations": {
    "sideBar.background": "#282C34",
    "list.hoverBackground": "#555555",
    "focusBorder": "#00000000",
    "editorGroupHeader.tabsBackground": "#282C34",
    "tab.border": "#00000000",
    "tab.activeBorder": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "scrollbar.shadow": "#00000000",
    "widget.shadow": "#00000000",
    "tab.inactiveBackground": "#282C34",
    "statusBar.background": "#282C34"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "filesFirst",
  "window.newWindowDimensions": "maximized",
  "workbench.startupEditor": "none",
  "git.autofetch": true,
  "cursor.cpp.disabledLanguages": ["markdown"],
  "custom-ui-style.font.sansSerif": "Berkeley Mono",
  "workbench.colorTheme": "Pastel Pro",
  "chatgpt.apiBase": "",
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.startup.launchAtLogin": true,
  "cursor.enable_agent_window_setting": true,
  "githubPullRequests.pullBranch": "never",
  "cursor.enable_git_worktrees_setting": true,
  "workbench.sideBar.location": "right",
  "git.blame.statusBarItem.enabled": true,
  "cursor.general.disableHttp2": true,
  "update.releaseTrack": "dev",
  "update.mode": "silentlyApplyOnQuit",
  "cursor.worktreeMaxCount": 100
}
